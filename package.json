{
  "name": "kubevious",
  "version": "0.0.9",
  "description": "Kubevious CLI",
  "keywords": [
    "kubernetes",
    "k8s",
    "validation",
    "validate",
    "lint",
    "security",
    "check",
    "sanity",
    "CRD"
  ],
  "main": "dist/index.js",
  "bin": {
    "kubevious": "./bin/kubevious"
  },
  "preferGlobal": true,
  "types": "dist",
  "files": [
    "dist/**/*",
    "assets/**/*"
  ],
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "build": "tsc",
    "format": "prettier --write ./src/ ./test/",
    "format-check": "prettier --write ./src/ ./test/",
    "lint": "eslint",
    "start": "tsc && node dist/index.js"
  },
  "author": "Ruben Hakopian <ruben.hakopian@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kubevious/cli/issues"
  },
  "homepage": "https://kubevious.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kubevious/cli"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "axios": "^0.21.4",
    "chalk": "^4.1.2",
    "cli-table": "^0.3.11",
    "commander": "^9.4.1",
    "express-openapi-validate": "^0.6.1",
    "glob": "^8.0.3",
    "js-yaml": "^4.1.0",
    "k8s-super-client": "^2.0.37",
    "kubernetes-types": "^1.25.0",
    "node-emoji": "^1.11.0",
    "openapi-schema-validator": "^12.0.2",
    "ora": "^5.4.1",
    "semver": "^7.3.8",
    "the-lodash": "^2.0.9",
    "the-logger": "^2.0.14",
    "the-promise": "^2.0.15",
    "uuid-parse": "^1.1.0"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.1",
    "@types/glob": "^8.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^8.0.3",
    "@types/node": "^17.0.23",
    "@types/node-emoji": "^1.8.2",
    "@types/semver": "^7.3.12",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "mocha": "^8.1.1",
    "prettier": "^2.1.0",
    "should": "^13.2.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "the-lodash": "^2.0.9",
    "the-logger": "^2.0.14",
    "the-promise": "^2.0.15",
    "k8s-super-client": "^2.0.37"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": "assets/**/*",
    "targets": [
      "node14-alpine-x64",
      "node14-alpine-arm64",
      "node14-linux-x64",
      "node14-linux-arm64",
      "node14-linuxstatic-x64",
      "node14-linuxstatic-arm64",
      "node14-win-x64",
      "node14-win-arm64",
      "node14-macos-x64",
      "node14-macos-arm64"
    ],
    "outputPath": "binary"
  }
}
